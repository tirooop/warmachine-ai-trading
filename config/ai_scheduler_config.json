{
    "ai_scheduler": {
        "enabled": true,
        "log_level": "INFO",
        "components": {
            "model_router": {
                "enabled": true,
                "model": "deepseek-ai/DeepSeek-V3",
                "max_tokens": 2000,
                "temperature": 0.7,
                "retry_attempts": 3,
                "timeout_seconds": 30,
                "version_control": {
                    "enabled": true,
                    "rollback_threshold": 0.8,
                    "update_interval_hours": 24
                }
            },
            "analyzer": {
                "enabled": true,
                "update_interval_seconds": 300,
                "analysis_types": [
                    "technical",
                    "sentiment",
                    "fundamental",
                    "orderflow"
                ],
                "alert_thresholds": {
                    "volatility": 0.02,
                    "volume_spike": 2.0,
                    "price_change": 0.01
                },
                "signal_quality": {
                    "min_quality_score": 0.8,
                    "min_confidence": 0.7,
                    "min_reliability": 0.6,
                    "validation_interval_seconds": 60
                }
            },
            "alert_generator": {
                "enabled": true,
                "channels": [
                    "telegram",
                    "email",
                    "webhook"
                ],
                "priority_levels": [
                    "low",
                    "medium",
                    "high",
                    "critical"
                ],
                "rate_limit_seconds": 60,
                "threshold_optimization": {
                    "enabled": true,
                    "update_interval_hours": 12,
                    "min_samples": 100
                }
            },
            "alert_factory": {
                "enabled": true,
                "templates_dir": "templates/alerts",
                "custom_templates": true,
                "max_alerts_per_minute": 10
            },
            "trading_manager": {
                "enabled": true,
                "max_positions": 10,
                "position_sizing": {
                    "type": "risk_based",
                    "max_risk_per_trade": 0.02
                },
                "risk_management": {
                    "stop_loss": true,
                    "trailing_stop": true,
                    "take_profit": true
                },
                "execution_quality": {
                    "min_success_rate": 0.9,
                    "max_slippage": 0.001,
                    "min_fill_rate": 0.95,
                    "metrics_interval_seconds": 300
                }
            },
            "feedback_learner": {
                "enabled": true,
                "learning_rate": 0.01,
                "batch_size": 32,
                "training_interval_hours": 24,
                "min_samples_required": 100,
                "metrics": [
                    "win_rate",
                    "profit_factor",
                    "sharpe_ratio",
                    "max_drawdown"
                ],
                "optimization": {
                    "enabled": true,
                    "update_interval_hours": 6,
                    "min_improvement": 0.01
                }
            }
        },
        "feedback_loop": {
            "global": {
                "enabled": true,
                "interval_seconds": 3600,
                "components": [
                    "analyzer",
                    "trading_manager",
                    "feedback_learner"
                ]
            },
            "local": {
                "signal_quality": {
                    "enabled": true,
                    "interval_seconds": 300,
                    "components": [
                        "analyzer",
                        "alert_generator"
                    ]
                },
                "execution_quality": {
                    "enabled": true,
                    "interval_seconds": 300,
                    "components": [
                        "trading_manager",
                        "feedback_learner"
                    ]
                }
            }
        },
        "integration": {
            "event_pool": {
                "enabled": true,
                "max_events": 1000,
                "cleanup_interval_seconds": 3600
            },
            "data_hub": {
                "enabled": true,
                "cache_size": 1000,
                "update_interval_seconds": 60
            },
            "notification": {
                "enabled": true,
                "retry_attempts": 3,
                "batch_size": 50,
                "rate_limit_seconds": 1
            }
        },
        "monitoring": {
            "enabled": true,
            "metrics_interval_seconds": 60,
            "health_check_interval_seconds": 300,
            "alert_on_component_failure": true,
            "performance_logging": true,
            "component_states": {
                "tracking_enabled": true,
                "state_history_size": 1000,
                "state_change_notification": true
            }
        },
        "storage": {
            "type": "file",
            "path": "data/ai",
            "backup_enabled": true,
            "backup_interval_hours": 24,
            "max_backup_count": 7
        }
    }
} 